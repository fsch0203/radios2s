{
    "background": {
        "service_worker": "js/background.js",
        "type": "module"
    },
    "action": {
        "default_icon": "s2s-icon-128.png"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    },
    "description": "Free Internet Radio Player",
    "homepage_url": "https://radios2s.scriptel.nl",
    "icons": {
        "16": "s2s-icon-16.png",
        "32": "s2s-icon-32.png",
        "48": "s2s-icon-48.png",
        "64": "s2s-icon-64.png",
        "128": "s2s-icon-128.png",
        "256": "s2s-icon-256.png",
        "512": "s2s-icon-512.png"
    },
    "manifest_version": 3,
    "name": "Radio Station-to-Station Ext",
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "version": "1.3.0"
}